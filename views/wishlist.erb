<div class="header">
  <h1>My Wishlist</h1>
</div>

<% @item.each do |item| %>
  <p>Item: <%= item.name %></p>
  <p>Price: $<%= item.price %></p>
  <img src="<%= item.pic %>" alt="My Item">

  <form action="/del/<%= item.id %>" method="post">
    <input type="hidden" name="_method" value="delete">
    <button>Delete</button>
  </form>

<% end %>

<a href="/users/<%= current_user.id %>">My Profile</a>
